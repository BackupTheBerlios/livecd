package main;

use lib qw(/usr/lib/libDrakX);
use common;

use Qt;
use fMain;

MAIN: {
	require_root_capability();

	my $a = Qt::Application(\@ARGV);
	my $w = fMain;

	$a->setMainWidget($w);
	$w->show;

	exit $a->exec;
}


